{
    "name": "ip-address-tracker",
    "version": "1.0.0",
    "description": "![Design preview for the IP address tracker coding challenge](./design/desktop-preview.jpg)",
    "main": "index.js",
    "scripts": {
        "watch": "sass --watch scss/main.scss public/css/main.css",
        "start": "live-server",
        "compile": "scss/main.scss scss/comp.css",
        "prefix": "postcss scss/comp.css -u autoprefixer -o scss/prefix.css",
        "compress": "sass scss/prefix.css public/css/main.css --style compressed",
        "build": "npm-run-all compile prefix compress"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^10.2.6",
        "live-server": "^1.2.1",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^8.3.1",
        "sass": "^1.35.1"
    }
}
